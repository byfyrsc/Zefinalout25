# Exemplo de arquivo .env para o InteliFeed Hub

# Configurações do Supabase
VITE_SUPABASE_URL="SUA_URL_SUPABASE_AQUI"
VITE_SUPABASE_ANON_KEY="SUA_CHAVE_ANON_SUPABASE_AQUI"

# Configurações do Stripe (para faturamento)
VITE_STRIPE_PUBLISHABLE_KEY="SUA_CHAVE_PUBLICAVEL_STRIPE_AQUI"
VITE_STRIPE_SECRET_KEY="SUA_CHAVE_SECRETA_STRIPE_AQUI" # Usada em Edge Functions
VITE_STRIPE_WEBHOOK_SECRET="SEGREDO_WEBHOOK_STRIPE_AQUI" # Usada em Edge Functions

# IDs de Preço do Stripe para os planos
VITE_STRIPE_STARTER_PRICE_ID="price_12345"
VITE_STRIPE_PROFESSIONAL_PRICE_ID="price_67890"
VITE_STRIPE_ENTERPRISE_PRICE_ID="price_abcde"

# URL da aplicação (para redirecionamentos do Stripe, etc.)
VITE_APP_URL="http://localhost:8080"

# Configurações de Desenvolvimento
# Defina como 'true' para usar autenticação mockada em desenvolvimento (pula o login real)
VITE_USE_MOCK_AUTH=true
# Defina como 'true' para usar dados mockados para componentes que dependem de dados do backend (ex: NPS Engine)
VITE_USE_MOCK_DATA=true